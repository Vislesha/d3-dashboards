{
  "name": "d3-dashboards-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build d3-dashboards",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "eslint . --ext .ts,.html --ignore-path .eslintignore --max-warnings=0",
    "format": "prettier --write \"{src,projects}/**/*.{ts,html,scss,css,json,md}\"",
    "serve": "ng serve",
    "clean": "node -e \"const fs=require('fs');['dist','out-tsc','coverage'].forEach((p)=>fs.rmSync(p,{recursive:true,force:true}));\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.2.0",
    "@angular/common": "20.2.0",
    "@angular/compiler": "20.2.0",
    "@angular/core": "20.2.0",
    "@angular/forms": "20.2.0",
    "@angular/platform-browser": "20.2.0",
    "@angular/platform-browser-dynamic": "20.2.0",
    "@angular/router": "20.2.0",
    "@primeng/themes": "20.0.0",
    "angular-gridster2": "^20.0.0",
    "d3": "^7.8.5",
    "lodash-es": "^4.17.21",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^18.3.1",
    "@angular-eslint/eslint-plugin-template": "^18.3.1",
    "@angular-eslint/template-parser": "^18.3.1",
    "@angular/build": "20.2.0",
    "@angular/cli": "20.2.0",
    "@angular/compiler-cli": "20.2.0",
    "@angular-devkit/build-angular": "20.2.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.1",
    "ng-packagr": "20.2.0",
    "prettier": "^3.0.0",
    "typescript": "~5.8.0"
  }
}